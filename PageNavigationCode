using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using UnityEngine.SceneManagement;

public class Navigation : MonoBehaviour
{
    public void StartUpPage()
    {
        SceneManager.LoadScene("StartUpPage");
    }
    public void HomePage()
    {
        SceneManager.LoadScene("HomePage");
    }
    public void SquatsPage()
    {
        SceneManager.LoadScene("Squats");
    }
    public void CurlsPage()
    {
        SceneManager.LoadScene("Curls");
    }
    
    public void PushUpsPage()
    {
        SceneManager.LoadScene("PushUps");
    }
}
